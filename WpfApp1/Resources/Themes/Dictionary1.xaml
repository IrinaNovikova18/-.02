<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:WpfApp1.Themes">
    <Style TargetType="{x:Type Button}">
        <Setter Property="Background" Value="#FFEEE266"></Setter>
        <Setter Property="BorderBrush" Value="#FFE17CE6"></Setter>
        <Setter Property="MinWidth" Value="123"></Setter>
        <Setter Property="Height" Value="321"></Setter>
    </Style>
    <Style TargetType="{x:Type DataGrid}">
        <Setter Property="Background" Value="#FFEEE266"></Setter>
        <Setter Property="BorderBrush" Value="#FFD7B7F7"></Setter>
    </Style>
    <Style TargetType="{x:Type Image}" x:Key="LogoImageStyle">
        <Setter Property="Source" Value="..\Images\images.png"></Setter>
        <Setter Property="Width" Value="100"></Setter>
        <Setter Property="Height" Value="100"></Setter>
        <Setter Property="Margin" Value="10, 10, 0, 0"></Setter>
        <Setter Property="HorizontalAlignment" Value="Left"></Setter>
        <Setter Property="VerticalAlignment" Value="Top"></Setter>
    </Style>
    <Style TargetType="{x:Type Window}" x:Key="windowStyle">
        <Setter Property="Background" Value="#FFE17CE6"></Setter>
        <Setter Property="FontFamily" Value="Times New Romsn"></Setter>
    </Style>
    
</ResourceDictionary>